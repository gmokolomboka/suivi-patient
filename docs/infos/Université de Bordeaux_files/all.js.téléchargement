var pathimg = '/bundles/nubcore/images/',
    	 	    pathbg  = pathimg + 'bg/',
    	 	    local   = 'localhost';

function cols(selector){

    if(!$(selector).length) return;
    
    if($('#page').hasClass('faq') || $('#page').hasClass('formulaire') || $('#page').hasClass('tiers-formulaire') || $('#page').hasClass('idex-formulaire') || $('#page').hasClass('tiers-faq'))
    {
        var introheight       = $(selector).height(),
            diagoheight       = $('#diago').height(),
            introtitleheight  = $(selector).find('h1').height() + $(selector).find('h2, p').height();
        
    
        $('#colcenter').css('top', (introheight + diagoheight) * -1);
        $('#colcenter').css('margin-bottom', (introheight + diagoheight) * -1);
        $('#colcenter').css('min-height', (introheight + diagoheight) + 20);
        $('#colcenter').css('padding-top', introtitleheight + 20);
        
        $('#colright').css('top', (introheight + diagoheight) * -1);
        $('#colright').css('padding-top', introtitleheight + 20);   }
    else
    {
        var introheight       = $(selector).height(),
            diagoheight       = $('#diago').height(),
            introtitleheight  = $(selector).find('h1').height();
        
        if(!$('#colright').hasClass('noTab')){
            $('#colright').css('top', (introheight + diagoheight - 10) * -1);
            $('#colright').css('margin-bottom', (introheight + diagoheight - 10) * -1);
            $('#colright').css('min-height', (introheight + diagoheight - 10) + 20);
            
            $('#colright').css('padding-top', introtitleheight);
        }


        $('#colleft').css('top', (introheight + diagoheight - 10) * -1);
        $('#colleft').css('margin-bottom', (introheight + diagoheight - 10) * -1);
        $('#colleft').css('min-height', (introheight + diagoheight - 10) + 20);
        
        $('#colleft').css('margin-bottom', (introheight + diagoheight) * -1);
    }
};

function equalize(selector)
{   
    var select, heights;
    
    this.data || (this.data = []);
    
    $(selector).each(function(){
    
        this.style.height = '';
        this.style.display = 'none';
        this.offsetHeight;
        this.style.display = '';    
    });

    if (!this.data.length) {
        var data = this.data;
        
        $(selector).each(function () {
            select = $(this).data('equalize');
        
            if (data.indexOf(select) == -1)
            {
                data.push(select);
            }
        });
    }

    for(var i = 0, len = this.data.length ; i < len ; i++)
    {
        select = selector + '[data-equalize=' + this.data[i] + ']';
        heights = $(select).map(function () { return $(this).height(); });
        $(select).height(Math.max.apply(null, heights));
    }
};


    
var oldstate, state;

function fluidcarrousel(selector){
    $(selector).each(function(){

        if($(this).attr('id') == 'slides')
        {
            var raw = $('#slides-container').html(),
                viewport = $(document).width(),
                largeur = $('#slides').width(),
                item,
                prev = $(this).attr('data-prev'),
                next = $(this).attr('data-next');

            if(viewport <= 499)
            {
                state = 'XXS';
                item = largeur;
            }
            else if(viewport <= 739)
            {
                state = 'XS';
                item = largeur;
            }
            else if(viewport > 739 && viewport <= 1023)
            {
                state = 'S';
                item = largeur / 2;
            }
            else
            {
                state = 'M';
                item = largeur / 2;
            }

            if (selector == '.flexsliderAccueil') {item = largeur;}

            $('#slides-container').html(raw);

            $('#slides').flexslider({
                slideshow:      false,
                directionNav:   true,
                controlNav:     false,
                slideshow:      true,
                slideshowSpeed: 5000,
                pauseOnAction:  true,
                touch:          false,
                itemWidth:      item,
                animation:      'slide',
                prevText:       '<span class="nav-prev" data-icon="prev" data-alt="'+ prev +'"></span>',
                nextText:       '<span class="nav-next" data-icon="next" data-alt="'+ next +'"  ></span>',
                start: function(flex){
                    if (selector == '.flexsliderAccueil') {equalize('.flexsliderAccueil .equalize');}
                    else {equalize('.flexslider .equalize');}

                    flex.find('.nav-prev').load(pathimg + flex.find('.nav-prev').attr('data-icon') +'.svg', function(){

                        flex.find('.nav-prev').append('<span class="mask">' + flex.find('.nav-prev').attr('data-alt') + '</span');
                    });

                    flex.find('.nav-next').load(pathimg + flex.find('.nav-next').attr('data-icon') +'.svg', function(){

                        flex.find('.nav-next').append('<span class="mask">' + flex.find('.nav-next').attr('data-alt') + '</span');
                    });
                }
            });

        }
        if($(this).attr('id') == 'slidesTwit')
        {
            var raw = $('#slidesTwit-container').html(),
                viewport = $(document).width(),
                largeur = $('#slidesTwit').width(),
                item,
                prev = $(this).attr('data-prev'),
                next = $(this).attr('data-next');

            if(viewport <= 499)
            {
                state = 'XXS';
                item = largeur;
            }
            else if(viewport <= 739)
            {
                state = 'XS';
                item = largeur;
            }
            else if(viewport > 739 && viewport <= 1023)
            {
                state = 'S';
                item = largeur / 2;
            }
            else
            {
                state = 'M';
                item = largeur / 2;
            }
            item = largeur;

            if(state != oldstate)
            {
                oldstate = state;

                $('#slidesTwit-container').html(raw);

                $('#slidesTwit').flexslider({
                    slideshow:      false,
                    directionNav:   true,
                    controlNav:     false,
                    slideshow:      true,
                    slideshowSpeed: 3000,
                    pauseOnAction:  true,
                    touch:          false,
                    itemWidth:      item,
                    animation:      'slide',
                    prevText:       '<span class="nav-prev" data-icon="prev" data-alt="'+ prev +'"></span>',
                    nextText:       '<span class="nav-next" data-icon="next" data-alt="'+ next +'"  ></span>',
                    start: function(flex){

                        equalize('.flexslider .equalize');

                        flex.find('.nav-prev').load(pathimg + flex.find('.nav-prev').attr('data-icon') +'.svg', function(){

                            flex.find('.nav-prev').append('<span class="mask">' + flex.find('.nav-prev').attr('data-alt') + '</span');
                        });

                        flex.find('.nav-next').load(pathimg + flex.find('.nav-next').attr('data-icon') +'.svg', function(){

                            flex.find('.nav-next').append('<span class="mask">' + flex.find('.nav-next').attr('data-alt') + '</span');
                        });
                    }
                });
            }
        }
    });
}

function carrousel(selector){

    $(selector).each(function(){
    
        var prev = $(this).attr('data-prev'),
            next = $(this).attr('data-next');
    
        if($(this).attr('id') == 'slides' || $(this).attr('id') == 'slidesTwit')
        {
            return;
        }
        else if($(this).attr('id') == 'carrousel')
        {
            var controlNav = false;
            
            if($(this).attr('data-controlNav') != undefined)
            {
                controlNav = $(this).attr('data-controlNav');
                
                controlNav = (controlNav === 'true') ? true : false;
            }
        
            $(this).flexslider({
                slideshow:      false,
                directionNav:   true,
                controlNav:     controlNav,
                slideshow:      true,
                slideshowSpeed: 10000,
                pauseOnAction:  true,
                touch:          false,
                animation:      'slide',
                prevText:       '<span class="prev" data-icon="prev" data-alt="'+ prev +'"></span>',
                nextText:       '<span class="next" data-icon="next" data-alt="'+ next +'"  ></span>',
                start: function(flex){
                    
                    flex.find('.prev').load(pathimg + flex.find('.prev').attr('data-icon') +'.svg', function(){
                    
                        flex.find('.prev').append('<span class="mask">' + flex.find('.prev').attr('data-alt') + '</span');
                    });
                    flex.find('.next').load(pathimg + flex.find('.next').attr('data-icon') +'.svg', function(){
                    
                        flex.find('.next').append('<span class="mask">' + flex.find('.next').attr('data-alt') + '</span');
                    });
                }
            });
        }
        else
        {
            var animationLoop = true,
                slideshow     = true,
                useCSS        = true,
                direction     = 'horizontal';
            
            if($(this).attr('data-animationLoop') != undefined)
            {
                animationLoop = $(this).attr('data-animationLoop');
                
                animationLoop = (animationLoop === 'true') ? true : false;
            }
            if($(this).attr('data-slideshow') != undefined)
            {
                slideshow = $(this).attr('data-slideshow');
                
                slideshow = (slideshow === 'true') ? true : false;
            }
            if($(this).attr('data-direction') != undefined)
            {
                direction = $(this).attr('data-direction');
            }
            if($(this).attr('data-useCSS') != undefined)
            {
                useCSS = $(this).attr('data-useCSS');
                
                useCSS = (useCSS === 'true') ? true : false;
            }
        
            $(this).flexslider({
                animationLoop:  animationLoop,
                slideshow:      slideshow,
                direction:      direction,
                directionNav:   true,
                controlNav:     false,
                slideshow:      true,
                slideshowSpeed: 10000,
                pauseOnAction:  true,
                touch:          false,
                animation:      'slide',
                useCSS:         useCSS,
                prevText:       '<span class="prev" data-icon="prev" data-alt="'+ prev +'"></span>',
                nextText:       '<span class="next" data-icon="next" data-alt="'+ next +'"  ></span>',
                start: function(flex){
                
                    flex.find('.prev').load(pathimg + flex.find('.prev').attr('data-icon') +'.svg', function(){
                    
                        flex.find('.prev').append('<span class="mask">' + flex.find('.prev').attr('data-alt') + '</span');
                    });
                    flex.find('.next').load(pathimg + flex.find('.next').attr('data-icon') +'.svg', function(){
                    
                        flex.find('.next').append('<span class="mask">' + flex.find('.next').attr('data-alt') + '</span');
                    });
                    
                    if(flex.hasClass('reverse'))
                    {
                        flex.flexslider(flex.count - 1);
                    }
                }
            });
        }
    });
    
};


function createCookie(name,value,days)
{
    if(days)
    {
        var date = new Date();
        date.setTime(date.getTime()+(days*24*60*60*1000));
        var expires = '; expires='+date.toGMTString();
    }
    else var expires = '';
    document.cookie = name+'='+value+expires+'; path=/';
}

function readCookie(name)
{
    var nameEQ = name + '=';
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++)
    {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}


var lisibilite = function(selector){
    
    if(!$(selector).length) return;
    
    var less = $(selector).attr('data-less'),
        more = $(selector).attr('data-more'),
        lisibility = $(selector).attr('data-lisibility');
        
    $(selector).append('' +
        '<p>'+ lisibility + '</p>' +
        '<ul id="text">' +
            '<li><button class="small" title="'+ less +'">A-</button></li>' +
            '<li><button class="big"   title="'+ more +'">A+</button></li>' +
        '</ul>' +
    '');
    
    var echelle = [0,1,2],
        taille  = echelle.length,
        pos     = 0,
        id;
        
    if(!window.localStorage){
        id = readCookie('lisibilite');
    }else{
        id = localStorage.getItem('lisibilite');
    }
    
    if(id){
        $('html').attr('id',id);
        pos = parseInt(id.replace('echelle-',''));
        
        if(pos == 0){
            $(selector).find('.small').addClass('disabled').attr('disabled','disabled');
        }else if(pos == (taille - 1)){
            $(selector).find('.big').addClass('disabled').attr('disabled','disabled');      
        }
    }

    $(selector).find('button').on('click', function(){
    
        $(selector).find('button').removeClass('disabled').removeAttr('disabled');
        
        if($(this).hasClass('small')){
            if(pos > 0){
                pos--;
            }
            if(pos == 0)
            {
                $(selector).find('.small').addClass('disabled').attr('disabled','disabled');
            }
        }else{
            if(pos < (taille - 1)){
                pos++;
            }           
            if(pos == (taille - 1)){
                $(selector).find('.big').addClass('disabled').attr('disabled','disabled');
            }
        }
        
        $('html').removeAttr('id')
                      .attr('id','echelle-'+ pos);

        if(!window.localStorage){
            createCookie('lisibilite','echelle-'+ pos, 365);
        }else{
            localStorage.setItem('lisibilite', 'echelle-'+ pos);
        }
    });
}


function lang(selector){

    $(selector).find('p').wrapInner('<button></button>');
    
    $(selector).find('button').click(function(e){
    
        e.preventDefault();
        e.stopPropagation();
        
        var that = $(this);
        
        if(that.hasClass('active'))
        {
            that.closest(selector).find('ul').slideUp(function(){
                that.removeClass('active');
            });
        }
        else
        {
            that.addClass('active');
            that.closest(selector).find('ul').slideDown();
            
            if($('#acces-search').find('.active').length)
            {
                $('#acces-search').find('.active').find('button').trigger('click');
            }
        }
    });
    $('body').on('click', function(e){
    
        $(selector).find('button').filter('.active').trigger('click');
    });
};

function nav(selector){

    var h0 = 0;

    $(selector).find('ul').filter(':first').find('> li').each(function(){
    
        if(!$(this).find('.button').find('a').length)
        {
            $(this).find('.button').wrapInner('<button></button>');
        }
    });
    
    $(selector).find('.button').find('button').on('click', function(e){
    
        e.preventDefault();
        e.stopPropagation();
        
        $(this).blur();
        
        var ssmenu = $(this).closest('li');
        
        $(selector).find('li').not(ssmenu).removeClass('active').find('.ssmenu').slideUp(300);
        
        if(ssmenu.hasClass('active'))
        {
            ssmenu.find('.ssmenu').slideUp(300, function(){
            
                $(this).closest('li').removeClass('active');
            });
        }
        else
        {
            ssmenu.addClass('active').find('.ssmenu').slideDown(300, 'swing', function(){
            
                if(h0 == 0)
                {
                    h0 = $(selector).find('.inside-nav').find('> ul').height();
                }
            });
            
            equalize('#nav .equalize');
        }
    });
    $(selector).find('.inside-alt, .inside, .inside-nav').on('click', function(e){
    
        e.stopPropagation();
    });
    $('body').on('click', function(e){
    
        $(selector).find('.active').find('.button').find('button').trigger('click');
    });
    $(selector).find('.inside-nav').find('> ul').find('> li').find('> a').on('click', function(e){

        if($(this).closest('li').find('ul').length)
        {

            e.preventDefault();

            //CLOSE ALL UL OPENED
            $.each($('.inside-nav li ul'), function(i, item){
                $(item).removeClass("active");
            });
            
            var that = $(this);
        
            that.closest('li').find('ul').addClass('active');
            
            var h = that.closest('li').find('ul').height();
            
            that.closest('ul').animate({
                marginLeft: '-100%'
            }, function(){
            
                that.closest('ul').height(h);
            });
        }
    });
    
    $(selector).find('.inside-nav').find('button').on('click', function(e){
        
        e.preventDefault();
        
        $(this).closest('.inside-nav').find('> ul').height(h0);
        
        $(this).closest('.inside-nav').find('> ul').animate({
            marginLeft: '0%'
        });
    });
};

function acces(selector){

    $(selector).find('.button').wrapInner('<button></button>');
    
    $(selector).find('button').on('click', function(e){
    
        e.preventDefault();
        e.stopPropagation();
        
        var ssmenu = $(this).closest('li');
        
        $(selector).find('li').not(ssmenu).removeClass('active').find('ul').slideUp(300);
        
        if(ssmenu.hasClass('active'))
        {
            ssmenu.removeClass('active').find('ul').slideUp(300);
        }
        else
        {
            $('#lang').find('p').find('button').filter('.active').trigger('click');
            $('#search').filter('.active').find('button').filter(':first').trigger('click');
            
            ssmenu.addClass('active').find('ul').slideDown(300);        
        }
    });
    $('body').on('click', function(e){
    
        $(selector).find('.active').find('button').trigger('click');
    });
};

function fixed(selector){

    if(!$(selector).length)
        return

    var elt    = $(selector),
        win    = $(window),
        active = 'active',
        test   = $('#nav-bis');

    var fixe = function(){
    
        var testTop = test.offset().top,
             winTop = win.scrollTop();
            
        if(winTop > testTop && !elt.hasClass(active))
        {
             elt.addClass(active);
            test.addClass(active);
        }
        else
        if(winTop <= testTop && elt.hasClass(active))
        {
             elt.removeClass(active);
            test.removeClass(active);
        }
    };
    
    win.bind('scroll resize', $.throttle(50, fixe)).trigger('scroll');
};

function more(selector){

    $(selector).removeClass('off').removeClass('on');
    $(selector).find('.action').remove();
    
    var first = $(selector).filter(':first'),
        more  = first.attr('data-more'),
        less  = first.attr('data-less');

    var min = parseInt(first.find('.txt').find('div').css('min-height')) +
              parseInt(first.find('.img').height());
              
    if(first.find('.title').css('position') != 'absolute')
    {
        min = min + parseInt(first.find('.title').height());
    }

    $(selector).each(function(){
    
        $(this).append('' +
            '<ul class="action">' +
                '<li><button><span>'+ more +'</span></button></li>' +
            '</ul>' +
        '');
    
        if($(this).height() > min)
        {       
            $(this).addClass('off');
            
            var diplome = $(this).closest(selector);
            
            diplome.find('.txt').find('div').css('height', 'auto');
            diplome.attr('data-div-height', diplome.find('.txt').find('div').height());
            diplome.find('.txt').find('div').css('height', '');
        }
    });
    $(selector).find('button').on('click', function(e){
    
        e.preventDefault();
        
        var diplome = $(this).closest(selector);
        
        if(diplome.hasClass('off'))
        {
            $(this).closest(selector).addClass('on');
        
            diplome.find('.txt').find('div').animate({
                height: diplome.attr('data-div-height')
            }, 300, function(){
                $(this).closest(selector).removeClass('off');
            });
            diplome.find('.action').find('span').text(less);
        }
        else
        {
            $(this).closest(selector).addClass('off');
        
            diplome.find('.txt').find('div').animate({
                height: '11.25em'
            }, 300, function(){
                $(this).closest(selector).removeClass('on');
                diplome.find('.txt').find('div').css('height', '');
            });
            diplome.find('.action').find('span').text(more);
        }
    });
};

function tabs(selector){
    
    $(selector).find('a').on('click', function(e){
    
        if($(document).width() > 739)
        {
            $(selector).find('a').each(function(){
                
                $(this).removeClass('active')
                $($(this).attr('href')).removeClass('active');
            });
            
            $(this).addClass('active');
            $($(this).attr('href')).addClass('active');
            $(this).blur();
            
            return false;
        }
    });
    
    if($(document).width() > 739 && !$(selector).find('a').filter('.active').length)
    {
        $(selector).find('a').filter(':first').trigger('click');
    }
    else if($(document).width() > 739 && $(selector).find('a').filter('.active').length)
    {
        $(selector).find('a').filter('.active').trigger('click');
    }
};

function scrolling(selector){

    var speed = 300;
    
    $(selector).on('click', function(e){
    
        e.preventDefault();
        
        if($(this).closest('div.index').not('#page').length)
        {
            return;
        }
        
        if($(this).closest('#tabs').length && $(document).width() > 739)
        {
            return;
        }

        var id = $(this).attr('href');
        
        if(id == '#')
        {
            return;
            // goTo('body');
        }
        else if($(this).closest('.filter').length)
        {
            indexTo(id);
        }
        else
        {
            goTo(id);
        }
    });
    
    function indexTo(ancre){

        // TODO déplacer la position du scroll en fonction de la lettre voulue
    }
    
    function goTo(ancre){
        
        var pos = $(ancre).offset().top;
        
        if(($('#nav-bis').offset().top < $(ancre).offset().top - $('#nav').height() - 5) && $(document).width() > 1023)
        {
            pos = pos - $('#nav').height() - 5;
        }
    
        $('html, body').animate({scrollTop: pos},
            speed,
            'swing',
            function()
            {
                $(ancre).attr('tabindex','-1');
                $(ancre).focus();
                $(ancre).removeAttr('tabindex');
                $(ancre).blur();
                
                if(ancre == '#search' || ancre == '#profil' || ancre == '#directs')
                {
                    if(!$(ancre).hasClass('active'))
                    {
                        $(ancre).find('button').filter(':first').trigger('click');
                    }
                }
            });
    }
};

function social(selector){

    if(!$(selector).length) return;

    var social = $(selector).attr('data-content').replace(/\[/g,'<').replace(/\]/g,'>').replace(/'/g,'"');

    $(selector).append(social);

    $('#social').find('button').each(function(){

        $(this).load(pathimg + $(this).attr('data-icon') +'.svg', function() {
            $(this).append('<span class="mask">' + $(this).attr('data-alt') + '</span');
            switch($(this).attr('data-alt')) {
                case 'Twitter': {
                    $(this).on('click', function(e){
                        window.open('https://twitter.com/share?url_page='+window.location, '_blank', 'width=700,height=500');
                    });
                    break;
                }
                case 'Facebook': {
                    $(this).on('click', function(e){
                        window.open(
                            'https://www.facebook.com/sharer/sharer.php?u='+window.location,
                            'facebook-share-dialog',
                            'width=626,height=436');
                    });
                    break;
                }
                case 'Google+': {
                    $(this).on('click', function(e){
                        window.open('https://plus.google.com/share?url='+window.location,
                            '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
                    });
                    break;
                }
                case 'Mail': {
                    $(this).on('click', function(e){
                        window.location = "mailto:?subject="+window.document.title+"&body="+window.location;
                    });
                    break;
                }
            }
        });
    });
    $('#social').find('button').filter(':last').on('click', function(e){

        e.preventDefault();
        window.print();
    });
};

function accordion(selector){

    $(selector).find('.h2').wrapInner('<button></button>');
    
    $(selector).find('button').addClass('inactive').on('click init', function(e){
    
        e.preventDefault();
        var that = $(this);
        
        $(this).closest('.menu').find('.inside').slideToggle(300, function(){
            
            if(that.hasClass('inactive'))
            {
                that.removeClass('inactive');
                
                setTimeout(function(){
                
                    that.addClass('active');

                    if(e.type != 'init')
                    {
                        that.closest('h2').attr('tabindex','-1');
                        that.closest('h2').focus();
                        that.closest('h2').removeAttr('tabindex');
                        that.closest('h2').blur();
                    }
                
                }, 100);
            }
            else
            {
                that.removeClass('active');
                
                setTimeout(function(){
                
                    that.addClass('inactive');
                    
                    that.closest('h2').attr('tabindex','-1');
                    that.closest('h2').focus();
                    that.closest('h2').removeAttr('tabindex');
                    that.closest('h2').blur();
                
                }, 100);
            }
            
        });     
    });
    
    $(selector).find('.menu').filter('.active').find('button').trigger('init');
}

function map(selector){

    if(!$(selector).length)
    {
        return;
    }
    
    var img = 'poi.png';

    var current_color = $('#page').attr('data-color');
    
    if(current_color == 'green')
    {
        img = 'poi-green.png';    
    }
    else
    if(current_color == 'pink')
    {
        img = 'poi-pink.png';    
    }
    else
    if(current_color == 'brown')
    {
        img = 'poi-brown.png';    
    }
    else
    if(current_color == 'slate')
    {
        img = 'poi-slate.png';    
    }
    else
    if(current_color == 'turquoise')
    {
        img = 'poi-turquoise.png';    
    }
    else
    if(current_color == 'orange')
    {
        img = 'poi-orange.png';    
    }
    else {
        img = 'poi.png'; 
    }

    var image = new google.maps.MarkerImage(pathimg + img, null, null, null, new google.maps.Size(27.5, 35));

    $(selector).each(function(){

        var map, marker;
        var place = new google.maps.LatLng($(this).attr('data-lat'), $(this).attr('data-long'));
        
        var geocoder= new google.maps.Geocoder();

        function initialize(that){

            map = new google.maps.Map(document.getElementById($(that).attr('id')),{

                mapTypeId: google.maps.MapTypeId.ROADMAP,
                center: place,
                zoom: parseInt($(that).attr('data-zoom')),
                disableDefaultUI: false,
                zoomControl: true
            });

            marker = new google.maps.Marker({
                position: place,
                icon: image,
                zIndex: 1000
            });

            marker.setMap(map);

            if ($('#daddr').val())
            {
                var infowindow = new google.maps.InfoWindow({
                    content: $('#daddr').val()
                });

                google.maps.event.addListener(marker, 'click', function() {
                    infowindow.open(map, marker);
                });

            }
        }

        if($(this).attr('id') == 'loc' && navigator.geolocation)
        {
            if(navigator.geolocation)
            {
                $('#iti').find('form').find('p').prepend('<button type="reset" class="btn">'+ $(this).attr('data-loc') +'</button><span class="or">' + $(this).attr('data-or') + '</span>');
            }

            function affichePosition(position){
            
                var start = new google.maps.LatLng(position.coords.latitude, position.coords.longitude);

                trace(start);
            }

            function calculePosition(position) {

                var start = $('#saddr').val();
                
                $('#saddr').on('keyup', function(){
                
                    $(this).removeClass('error');
                });

                var geo = new google.maps.Geocoder;
                
                geo.geocode({'address': start},function(results, status){

                    if(status == google.maps.GeocoderStatus.OK){
                        var startlatlong = results[0].geometry.location;

                        trace(start);
                    }
                    else
                    {
                        $('#saddr').addClass('error');
                        
                        $('#loc').removeClass('load');
                    }
                });


            }

            function trace(start){
            
                var directionsDisplay;
                var directionsService = new google.maps.DirectionsService();

                function initialize(start){

                    directionsDisplay = new google.maps.DirectionsRenderer({markerOptions: {icon: image}});
                    marker.setMap(null);
                    directionsDisplay.setMap(map);
                    
                    // TODO : effacer les éventuels itinéraires précédants
                    
                    calcRoute(start);
                }

                function calcRoute(start){
                
                    var request = {
                        origin:      start,
                        destination: place,
                        travelMode: google.maps.DirectionsTravelMode.DRIVING
                    };

                    directionsService.route(request, function(response, status){
                        if(status == google.maps.DirectionsStatus.OK){
                        
                            $('#loc').removeClass('load');
                        
                            directionsDisplay.setDirections(response);
                        }
                    });
                    
                    var geocoder = new google.maps.Geocoder();

                    var href  = 'http://maps.google.com/maps?';
                    var daddr = $('#daddr').val();
                    
                    if(start.nb != undefined && start.ob != undefined)
                    {
                        var latlng = new google.maps.LatLng(start.nb, start.ob);
                                            
                        var saddr = '';
    
                        geocoder.geocode({'latLng': latlng}, function(results, status){
                        
                            if(status == google.maps.GeocoderStatus.OK)
                            {
                                if(results[1])
                                {
                                    saddr = '' + results[1].formatted_address;
                                    
                                    href = href + 'saddr=' + saddr + '&' + 'daddr=' + daddr;
                                    
                                    $('#iti').find('h2').find('a').attr('href', href);
                                }
                            }
                        });
                    }
                    else
                    {
                        saddr = start;
                        
                        href = href + 'saddr=' + saddr + '&' + 'daddr=' + daddr;
                        
                        $('#iti').find('h2').find('a').attr('href', href);
                    }
                }

                google.maps.event.addDomListener(window, 'load', initialize(start));
            }

            $('#iti').find('button').filter(':first').on('click', function(e){

                e.preventDefault();
                
                navigator.geolocation.getCurrentPosition(affichePosition);
            });

            $('#iti').find('button').filter(':last').on('click', function(e){

                e.preventDefault();
                
                calculePosition();
            });
            
            $('#iti').find('button').on('click', function(){
            
                $('#loc').addClass('load');
            
            });
        }

        google.maps.event.addDomListener(window, 'load', initialize(this));
    });
}

function validation(selector){

    $(selector).each(function(){
    
        $(this).validate({
            errorElement: 'span',
            errorPlacement: function(error, element){
                error.appendTo(element.closest('label'));
            }
        });
    });
}

function initsearch(selector)
{
    if($(selector).hasClass('active'))
    {   
        $(selector).find('form').animate({
            width: 0
        }, 300, function(){
        
            $(selector).removeClass('active');
        });
    }
}

function search(selector){

    $(selector).find('.button').wrapInner('<button></button>');
    $(selector).find('button').filter(':first').on('click', function(e){
    
        e.preventDefault();
        
        if($(selector).hasClass('active'))
        {   
            initsearch(selector);
        }
        else
        {
            $('#lang').find('p').find('button').filter('.active').trigger('click');
        
            $(selector).addClass('active');
            
            $('#acces').find('> li').filter('.active').find('button').trigger('click');
            
            $(selector).find('form').animate({
                width: $(selector).find('form').find('p').width()
            }, 300, function(){
            
                $(selector).find('input').focus();
            });
        }
    });
    
    if($(selector).find('input').val() != '')
    {
        $(selector).addClass('active');
        $(selector).find('form').width($(selector).find('form').find('p').width());
    }
}

function glossaire(selector){

    if(!$(selector).length) return;

    var viewport = $(document).width();

    if(viewport <= 499)
    {
        state = 'XXS';
    }
    else if(viewport <= 739)
    {
        state = 'XS';
    }
    else if(viewport > 739 && viewport <= 1023)
    {
        state = 'S';
    }
    else
    {
        state = 'M';
    }

    if(state != oldstate)
    {
        oldstate = state;

        //if(!$(selector).find('.filter').find('form').length)
        //  {
        //    var filtre = $(selector).attr('data-filtre').replace(/\[/g,'<').replace(/\]/g,'>').replace(/'/g,'"');

        //     $(selector).find('.filter').prepend(filtre);

        // TODO : ajouter le système de filtres sur le champ de recherche
        //  }



        $(selector).find('fiche').attr('style', '');

        var hash = document.location.hash.replace('link-','');

        if(state == 'XXS' || state == 'XS')
        {
            $(selector).find('.index').find('a').off('click').on('click', function(e){

                e.preventDefault();

                var that  = $(this),
                    fiche = $(that.attr('href'));

                that.toggleClass('active-alt');
                fiche.slideToggle();
                if ($('#title_'+that.attr('id')).html().trim() == ""){
                    var results = $('#results');
                    results.stop().fadeTo(300, 0.25);
                    results.addClass('load');
                    $.post('/glossary_item',
                        {locationId: that.attr('id')},
                        function(response){
                            var results = $('#results');
                            results.stop().fadeTo(300, 1);
                            results.removeClass('load');
                            $('#title_'+that.attr('id')).html(response.word.text);
                            $('#content_'+that.attr('id')).html(response.definition);
                        }, "json");
                }
            });
            $(selector).find('.filter').find('a').off('click').on('click', function(e){
                e.preventDefault();
                var lettre = $(this).attr('href');
                $('html,body').animate({scrollTop: $(lettre).offset().top}, 300);
                // TODO : déplacer le scroll de l'index visible au bon endroit
            });
        }
        else
        {
            $(selector).find('.index').find('a').off('click').on('click', function(e){

                e.preventDefault();

                var that  = $(this),
                    index = that.closest('.index'),
                    fiche = $(that.attr('href'));

                index.find('a').removeClass('active');
                index.find('.fiche').removeClass('active');

                that.addClass('active');
                fiche.addClass('active');

                if ($('#title_'+that.attr('id')).html().trim() == ""){
                    var results = $('#results');
                    results.stop().fadeTo(300, 0.25);
                    results.addClass('load');
                    $.post('/glossary_item',
                        {locationId: that.attr('id')},
                        function(response){
                            var results = $('#results');
                            results.stop().fadeTo(300, 1);
                            results.removeClass('load');
                            $('#title_'+that.attr('id')).html(response.word.text);
                            $('#content_'+that.attr('id')).html(response.definition);
                        }, "json");
                }
                if(index.is(':visible'))
                {
                    document.location.hash = 'link-' + that.attr('href').slice(1);
                }
            });

            var links = $(selector).find('.index').filter(':visible').find('a');

            if(!$(selector).find('.index').find('a').filter('.active').length)
            {
                $(selector).find('.index').each(function(){

                    $(this).find('a').filter(':first').trigger('click');
                });
            }

            $(selector).find('#tabs').find('a').on('click', function(e){
                var links = $(selector).find('.index').filter(':visible').find('a');

                if(!links.filter('.active').length)
                {
                    links.filter(':first').trigger('click');
                }
                else
                {
                    links.filter('.active').trigger('click');
                }
            });

            if(hash)
            {
                var tab = $(hash).closest('.tab');

                $('#tabs').find('a').each(function(){

                    if($(this).attr('href').slice(1) == tab.attr('id'))
                    {
                        if(!$(this).hasClass('active'))
                        {
                            $(this).trigger('click');
                            return;
                        }

                    }
                });

                $('.index').filter(':visible').find('a').each(function(){

                    if($(this).attr('href') == hash)
                    {
                        if(!$(this).hasClass('active'))
                        {
                            $(this).trigger('click');
                            return;
                        }
                    }
                });
            }
            $(selector).find('.filter').find('a').off('click').on('click', function(e){
                e.preventDefault();
                var lettre = $(this).attr('href');
                var thisone = $( "li[id="+lettre.substring(1,2)+"]" ).find('ul > li > a:first');
                var monlien = thisone.attr('id');
                var that  = thisone,
                    index = that.closest('.index'),
                    fiche = $(that.attr('href'));
                index.find('a').removeClass('active');
                index.find('.fiche').removeClass('active');

                that.addClass('active');
                fiche.addClass('active');
                var results = $('#results');
                results.stop().fadeTo(300, 0.25);
                results.addClass('load');
                $.post('/glossary_item',
                    {locationId: that.attr('id')},
                    function(response){
                        var results = $('#results');
                        results.stop().fadeTo(300, 1);
                        results.removeClass('load');
                        $('#title_'+that.attr('id')).html(response.word.text);
                        $('#content_'+that.attr('id')).html(response.definition);
                    }, "json");
                $('.index-alt').animate({scrollTop: 0}, 0);
                $('.index-alt').animate({scrollTop: $(lettre).position().top}, 300);
            });
        }
    }
}

function annuaire(selector){

    if(!$(selector).length) return;

    var viewport = $(document).width();

    if(viewport <= 499)
    {
        state = 'XXS';
    }
    else if(viewport <= 739)
    {
        state = 'XS';
    }
    else if(viewport > 739 && viewport <= 1023)
    {
        state = 'S';
    }
    else
    {
        state = 'M';
    }
    
    if(state != oldstate)
    {
        oldstate = state;
        
        if(!$(selector).find('.filter').find('form').length)
        {
            var filtre = $(selector).attr('data-filtre').replace(/\[/g,'<').replace(/\]/g,'>').replace(/'/g,'"');
        
            $(selector).find('.filter').prepend(filtre);
            
            // TODO : ajouter le système de filtres sur le champ de recherche
        }
        
        /*$(selector).find('.filter').find('a').off('click').on('click', function(e){
        
            e.preventDefault();
            
            // TODO : déplacer le scroll de l'index visible au bon endroit
        });*/
        
        $(selector).find('fiche').attr('style', '');
        
        //var hash = document.location.hash.replace('link-','');
        
        if(state == 'XXS' || state == 'XS')
        {
            $(selector).find('.index').find('a').off('click').on('click', function(e){
                
                e.preventDefault();
                
                var that  = $(this),
                    fiche = $(this).next('.fiche');
            
                that.toggleClass('active-alt');
                fiche.slideToggle();
            });
        }
        else
        {
            $(selector).find('.index').find('a').off('click').on('click', function(e){
            
                e.preventDefault();
                
                var that  = $(this),
                    index = that.closest('.index'),
                    fiche = $(this).next('.fiche');
            
                index.find('a').removeClass('active');
                index.find('.fiche').removeClass('active');
                    
                that.addClass('active');
                fiche.addClass('active');
                
                // if(index.is(':visible'))
                // {
                //  document.location.hash = 'link-' + that.attr('href').slice(1);
                // }
            });
            
            var links = $(selector).find('.index').filter(':visible').find('a');
                
            if(!$(selector).find('.index').find('a').filter('.active').length)
            {
                $(selector).find('.index').each(function(){
                
                    $(this).find('a').filter(':first').trigger('click');    
                });             
            }
            
            $(selector).find('#tabs').find('a').on('click', function(e){
            
                var links = $(selector).find('.index').filter(':visible').find('a');
            
                if(!links.filter('.active').length)
                {
                    links.filter(':first').trigger('click');
                }
                else
                {
                    links.filter('.active').trigger('click');
                }
            });
            
            // if(hash)
            // {    
            //  var tab = $(hash).closest('.tab');
                
            //  $('#tabs').find('a').each(function(){
                
            //      if($(this).attr('href').slice(1) == tab.attr('id'))
            //      {
            //          if(!$(this).hasClass('active'))
            //          {
            //              $(this).trigger('click');
            //              return;
            //          }
                        
            //      }
            //  });
                
            //  $('.index').filter(':visible').find('a').each(function(){
                        
            //      if($(this).attr('href') == hash)
            //      {
            //          if(!$(this).hasClass('active'))
            //          {
            //              $(this).trigger('click');
            //              return;
            //          }
            //      }
            //  });
            // }
        }
    }
}

function resume(selector){

    var sommaire = $(selector).attr('data-summary');

    $(selector).find('.summary').prepend('<button><span>'+ sommaire +'</span></button>');
    
    $(selector).find('button').on('click', function(e){
    
        e.preventDefault();
        e.stopPropagation();
        
        var that = $(this);
        
        if($(this).hasClass('active'))
        {
            that.closest('li').find('ul').slideUp(function(){
            
                that.removeClass('active');
//              that.text('Voir le sommaire');
            });
        }
        else
        {
            that.closest('li').find('ul').slideDown(function(){
            
                that.addClass('active');
//              that.text('Réduire le sommaire');
            });
        }
    });
    
    $('body').on('click', function(e){
    
        $(selector).find('button').filter('.active').trigger('click');
    });
    
    $(selector).find('ul').find('a').on('click', function(e){
    
        $(selector).find('button').trigger('click');
        
        if($(this).attr('href').indexOf('#accordion-') == 0)
        {
            that = $(this);
        
            setTimeout(function(){
            
                $(that.attr('href')).find('button').trigger('click');
            }, 0);
        }
    });
}

function autosubmit(selector){

    var timer = 500,
        clock,
        form    = $(selector).find(':input').closest('form'),
        state   = form.serialize();
    
    form.find(':input').on('change', function(){
    
        stateChange();
    });
        
    function stateChange(){
    
        clearTimeout(clock);
        
        clock = setTimeout(function(){
        
            if(state != form.serialize())
            {
                form.submit(); // ou ajax ?
            }
            
            state = form.serialize();
        
        }, timer);
    }
}

function filtre(selector){

    $(selector).find('select').on('change', function(){
    
        $(selector).find('.label').text($(selector).find('select').find('option').filter(':selected').text());
        
    }).trigger('change');
}

function titles(selector){

    $(selector).find('button').removeClass('active');
    $(selector).next('#filtres').attr('style', '');

    if(!$(selector).find('button').length)
    {
        $(selector).find('h2').wrapInner('<button></button>');
    }
    $(selector).find('button').off('click').on('click', function(e){
    
        e.preventDefault();
    
        if($(document).width() > 1024)
        {
            return;
        }
        
        if($(this).closest('.change').length)
        {
            $('#plage').find('button').trigger('click');
        }
        else
        {
            $(this).toggleClass('active');
            
            $(this).closest(selector).next('#filtres').slideToggle();
        }
    });
}

function filtres(selector){

    var timer = 2000,
        clock,
        form    = $(selector).closest('form'),
        state   = form.serialize(),
        results = $('#results'),
        invert  = $(selector).attr('data-invert');
        
    $(selector).find('.filtres').each(function(){
    
        var that   = $(this),
            p      = that.find('p').filter(':first');
    
        p.find('span').wrapInner('<button></button>');
        
        p.find('button').attr('data-text', $(this).find('button').text());
        
        p.find('button').on('click', function(e){
        
            e.preventDefault();
            
            $(this).toggleClass('active');
            
            $(this).closest('.filtres').find('.ssnav').slideToggle();
        });
        
        if(that.find(':checkbox').length)
        {
            $(this).find('.inside').prepend('' + 
                '<p class="invert">' +
                    '<button>'+ invert +'</button>' +
                '</p>' +
            '');
        }
        
        that.find(':input').not('button').on('change', function(e){
        
            var nb  = $(this).closest('.filtres').find(':checked').not('option').length,
                btn = $(this).closest('.filtres').find('p').filter(':first').find('button');
                
            if($(this).closest('.filtres').find('select').val() != 0 && $(this).closest('.filtres').find('select').val() != undefined)
            {
                nb = 1;
            }
                
            btn.text(btn.attr('data-text') + ' (' + nb + ')');
        });
        
        that.find(':input').not('button').filter(':first').trigger('change');
        
        that.find('.ssnav').find('button').on('click', function(e){
            
            e.preventDefault();
            
            $(this).closest('.ssnav').find(':checkbox').each(function(){
            
                $(this).trigger('click');
                
                that.find(':input').not('button').filter(':first').trigger('change');
            });
        });
    });
    
    form.find(':checkbox, :radio').not('button').on('click', function(){

        stateChange();
    });
    form.find('select').on('change', function(){
    
        stateChange();
    });
    
//  form.find('.calendar').on('change', function(){
//  
//      stateChange();
//  });
    
    function stateChange(){
    
        results.find('.inside').stop().fadeTo(300, 0.25);
        results.addClass('load');
    
        clearTimeout(clock);
        
        clock = setTimeout(function(){
        
            if(state != form.serialize())
            {
                form.submit(); // ou ajax ?
            }
            else
            {             
                results.find('.inside').stop().fadeTo(300, 1);
                results.removeClass('load');
            }
            
            state = form.serialize();
        
        }, timer);
    }
}

function trouver(selector){

//  bug chrome mobile qui déclenche le refresh trop souvent
//  $('#formations').attr('style', '');
//  $(selector).removeClass('active');

    if(!$(selector).find('h2').find('button').length)
    {
        $(selector).find('h2').wrapInner('<button></button>');
    }
    
    $(selector).find('button').off('click').on('click', function(e){
    
        e.preventDefault();
        var that = $(this);
        
        if($(selector).hasClass('active'))
        {
            $('#formations').slideUp(function(){
                that.closest(selector).removeClass('active');
                
                
            }); 
        }
        else
        {
            that.closest(selector).addClass('active');
            
            $('#formations').slideDown(function(){

                $('#formations').find('form').addClass('fixios');
            });
        }
    });
    
    if(!$(selector).closest('#blocs4').length)
    {
        $('#formations').find('select, input').on('change', function(){
        
            $(this).trigger('change-alt');
        });
    
        $('#formations').find('select, input').off('change-alt').on('change-alt', function(){
        
            var nb = 0;
            
            $('#formations').find('select, input').each(function(){
            
                if($(this).val() != '' && $(this).val() != 0)
                {
                    nb++;
                }
            });
            
            $(selector).find('h2').find('button').find('span').find('span').text('(' + nb + ')');
        
        }).trigger('change-alt');
    }
}

function actualites(selector){

    $(selector).find('.title').find('div').find('span').wrapInner('<button></button>');
    
    $(selector).find('button').on('click', function(){
    
        $(this).toggleClass('active').closest(selector).find('.title').find('ul').slideToggle();
    });
}

function select(selector){

    $(selector).find('.button').wrapInner('<button></button>');
    
    $(selector).find('.button').find('button').on('click', function(){
    
        $(this).toggleClass('active').closest(selector).find('.ssmenu').slideToggle();
    });
    
    var print = $(selector).attr('data-print');
    
    $(selector).find('.ssmenu').find('.cta').append('<li><button class="btn alt">'+ print +'</button></li>');
    
    $(selector).find('.cta').find('button').on('click', function(e){
    
        e.preventDefault();
        
        $('html').removeClass('full').addClass('selection');

        window.print();
        
        $('html').removeClass('selection').addClass('full');
    });
    
    $(selector).find('ul').find('ul').each(function(){
    
        $(this).find('a').filter(':last').on('click', function(e){
        
            e.preventDefault();
            
            var nb  = $(selector).find('button').find('span'),
                val = nb.find('span').text();
                
            val--;
                
            nb.find('span').text(val);
            
            if(val == 0)
            {
                var html = nb.html();
                nb.html(html.replace('formations', 'formation'));
            }
            
            $(this).closest('ul').closest('li').slideUp(function(){
            
                if(!$(this).closest('ul').find('li').filter(':visible').length)
                {
                    var that = $(this),
                        ul   = that.closest('ul'),
                        p    = $(selector).find('.ssmenu').find('p');
                    
                    ul.next('.cta').slideUp(function(){
                    
                        ul.next('.cta').remove();
                        ul.remove();
                        
                        p.slideUp(function(){
                        
                            p.text(p.attr('data-empty'));
                            
                            p.slideDown();
                        });
                        
                        
                    });
                }
                else
                {
                    $(this).remove();
                }
                
                var lien    = $(selector).find('.cta').find('a'),
                    mailto  = 'mailto:?',
                    subject = 'subject=' + lien.attr('data-subject'),
                    body    = '&body='   + lien.attr('data-body') + '%0A%0A',
                    formations = '';
                    
                if($(selector).find('ul').filter(':first').find('> li').length == 1)
                {
                    body    = '&body='   + lien.attr('data-body-single') + '%0A%0A';
                    subject = 'subject=' + lien.attr('data-subject-single');
                }
                    
                $(selector).find('ul').filter(':first').find('> li').each(function(){
                
                    var lien = $(this).find('a').filter(':first');
                    
                    formations = formations + '%20%E2%80%93%20' + lien.text() + ' (' + lien.attr('href') + ').' + '%0A';
                });
                    
                lien.attr('href', mailto + subject + body + formations);    
                
                // todo : ajax          
            });
        });
    });
}

function fast(selector){

    if(!$(selector).closest('form').length) return;

    $(selector).find('h3').find('span').wrapInner('<button></button>');
    
    $(selector).find('h3').find('button').on('click', function(e){
    
        e.preventDefault();
        
        $(this).closest(selector).toggleClass('active');
    });
    
    var catalogue = $('#catalogue').find('.catalogue').find('li'),
        filtres   = $(selector).closest('form').find('input');
    
    $(selector).closest('form').find('input').on('click init', function(){
    
        catalogue.removeClass('inactive');
        
        filtres.each(function(){
        
            if($(this).is(':checked'))
            {
                var filtre = $(this).attr('id');
                
                catalogue.each(function(){
                
                    if($(this).attr('data-filtre') != undefined)
                    {
                        if($(this).attr('data-filtre').indexOf(filtre) == -1)
                        {
                            $(this).addClass('inactive');
                        }
                    }               
                }); 
            }
        });
        
        if((catalogue.filter(':visible').length != catalogue.length) && !$(selector).hasClass('active'))
        {
            $(selector).find('h3').find('button').trigger('click');
        }
        
    }).trigger('init');

}

function calendar(selector)
{
    var change = $(selector).attr('data-text');
    
    var current_color = $('#page').attr('data-color');
    img = 'calendar-'+current_color+'.svg';  


    $(selector).closest('#plage').find('.dates').append('<button data-after="' + $(selector).attr('data-text') + '"><img src="' + pathimg + img + '" width="40" height="40" alt="" /></button');
    
    var btn = $(selector).closest('#plage').find('.dates').find('button');
    
    $(selector).closest('#plage').find('.dates').find('button').on('click', function(e){
    
        e.preventDefault();
    
        if($(selector).hasClass('hasDatepicker'))
        {
            $(selector).removeClass('hasDatepicker').html('');
        }
        else
        {
            var init = $(selector).attr('data-value');
        
            $(selector).datepicker({
                
                onSelect: function(){
                
                    if($(this).val() == init)
                    {
                        btn.trigger('click');
                    }
                    else
                    {
                        var loc = document.location + '';
                        
                        loc = loc.replace(/\&date=.{10}/, '');
                        var operande = '?';
                        if (loc.indexOf('?') > 0) {
                            operande = '&';
                        }
                        loc = loc + operande + 'date=' + $(this).val().replace(/\//g,'-');


                        document.location = loc; // TODO : adapter en dev
                    }
                }
            });
            $(selector).datepicker('setDate', $(selector).attr('data-value'));
        }
    });
}

function intro(selector)
{
    var h0 = 0;
    
    $(selector).find('h1').find('button').on('click', function(e){
    
        e.preventDefault();
        
        $(this).toggleClass('active').closest('h1').next('#menu-alt').slideToggle(function(){
        
        
            if(h0 == 0)
            {
                h0 = $(selector).find('#menu-alt').find('.inside').find('> ul').height() -1;
            }
        });
    });
    
    $(selector).find('#menu-alt').find('.inside').find('> ul').find('> li').find('> a').on('click', function(e){
    
        if($(this).closest('li').find('ul').length)
        {
            e.preventDefault();
            
            var that = $(this);
        
            that.closest('li').find('ul').addClass('active');
            
            var h = that.closest('li').find('ul').height();
            
            that.closest('ul').animate({
                marginLeft: '-100%'
            }, function(){
            
                that.closest('ul').height(h);
            });
        }
    });
    
    $(selector).find('#menu-alt').find('.inside').find('button').on('click', function(e){
        
        e.preventDefault();
        
        $(this).closest('.inside').find('> ul').height(h0);
        
        $(this).closest('.inside').find('> ul').animate({
            marginLeft: '0%'
        });
    });
}

function fontresize(selector){

    var interval = 200,
        id       = '#detectsize',
        event    = 'fontresize',
        mask     = 'mask';

    var elt = $('<span>&nbsp;</span>');
    
    elt.attr('id', id.substr(1));
    elt.addClass(mask);
    elt.css('height','auto');
    elt.appendTo(selector);

    var oldheight = $(id).height();
    
    setInterval(function(){
    
        var newheight = $(id).height();
        
        if(newheight != oldheight)
        {
            $(window).trigger(event);
            oldheight = newheight;
        }
        
    }, interval);
}

function campus(selector)
{
    $(selector).find('.menu').find('p').find('span').wrapInner('<button></button>');
    
    $(selector).find('button').on('click', function(e){
    
        e.preventDefault();
        
        $(this).toggleClass('active');
        $(this).closest('.menu').find('.ssmenu').slideToggle();
    });
}

function logo(selector)
{   
    if($(html).hasClass('no-touch') && $(selector).find('img').length && $(selector).attr('data-anim') != undefined)
    {
        var old = $(selector).html(),
            pos = old.search('<span class="mask">');
            
        $(selector).load(pathimg + $(selector).attr('data-anim'), function(){
        
            if(pos != -1)
            {
                $(selector).append(old.slice(pos));
            }
        });
    }
}

function refresh(){
    
    cols('#intro');
    equalize('.equalize');
    fluidcarrousel('.flexslider');
    more('#blocs3 .diplome');
    //initsearch('#search');
    tabs('#tabs');
    glossaire('#glossaire');
    annuaire('#annuaire');
    titles('.titles');
    trouver('#trouver');
}

$(window).load(function(){

    carrousel('.flexslider');
    lisibilite('#tools .left');
    lang('#lang');
    nav('#nav');
    acces('#acces');
    fixed('#nav');
    scrolling('a[href^="#"]');
    social('#social-container');
    accordion('.accordion');
    map('.map');
    validation('.validation');
    resume('#resume');
    autosubmit('.autosubmit');
    filtre('#filtre');
    filtres('#filtres');
    actualites('#actualites');
    calendar('.calendar');
    fast('#fast');
    select('#select');
    intro('#intro');
    fontresize('body');
    campus('#campus');
    logo('#header .h1 a');

    $(window).on('resize fontresize', $.throttle(200, refresh)).trigger('resize');

    search('#search');
    
    //$('html').addClass('loaded');
});
$(document).ready(function() {

	// contact form
	$('.form-training').validate({
		errorElement: 'span',
		errorPlacement: function(error, element){
			error.appendTo(element.closest('label'));
		},
		submitHandler: function(form) {

			if(typeof(url_contact_training_ajax)=='undefined'){ 
				console.log('You must define the route for the ajax form submit with variable "url_contact_training_ajax"');
				return;
			}

			$(form).stop().fadeTo(300, 0.25);
		    $(form).addClass('load');

			$.ajax({
				url: url_contact_training_ajax,
				type: 'POST',
				data: $(form).serialize()
			})
			.done(function(json) {
					$('.form-training').slideUp(400, function(){
					$('.contact-success').fadeIn();
				})
			})
			.fail(function( jqXHR, textStatus, errorThrown) {
				if(jqXHR.status != 400) {
					alert('Erreur de la requête : ' + errorThrown);
				} else {
					alert(jqXHR.responseText);
				}
				$(form).stop().fadeTo(300, 1);
	        	$(form).removeClass('load');
			});

			return false;
		}
	});


	// search form on catalog 	
	$('#form-recherche-formation').submit(function(){
		if( $('#champ_disciplinaire').val() == 0 &&
			$('#type_diplome').val() == 0 &&
			$('#discipline').val() == 0
		) {
			$('#error-msg-search-form').hide();
			$('#error-msg-search-form').fadeIn();
			return false;
		}
	});

	$('#champ_disciplinaire, #type_diplome, #discipline').change(function(){
		if($(this).val() != 0) {
			$('#error-msg-search-form').hide();
		}
	});


	$('#button-back').click(function(){
		var chaine = document.URL ;
		var position = chaine.indexOf(document.referrer);
		if (position == 0 && document.referrer!='') {
			window.history.back();
		}
		else {
			var tab = chaine.split(/\/enseignement/) ;
			window.location.href=tab[0];
		}
	});

	


});
function directory (selector) {

    if(!$(selector).length) return;

    function fadeOut(){
        var results = $('#results');
        results.stop().fadeTo(300, 0.25);
        results.addClass('load');
    }

    function fadeIn(){
        var results = $('#results');
        results.stop().fadeTo(300, 1);
        results.removeClass('load');
    }

    function loadList(letter){
        fadeOut();
        if (letter == undefined)
        {
            letter = 'a'
        }

        $.ajax({
            url: "/ldap/list/" + letter,
            context: document.body
        }).done(function(data) {
                renderList(data.letter, data.results);
            });
    }

    function loadUserInfos(target){
        fadeOut();
        var uid = target.attr('data-uid');

        if (uid){
            $.ajax({
                url: "/ldap/user/infos/" + uid,
                context: document.body
            }).done(function(data) {
                    renderUserInfos(data, target);
                });
        }
    }

    function search(searchText){
        fadeOut();

        $.ajax({
            url: "/ldap/search/" + searchText,
            context: document.body
        }).done(function(data) {
                renderList(null, data);
            });
    }

    function renderList(letter, data){
        var letterList = $('<ul>');
        if (letter != null) {
            var letterElement = $('<li>').attr('id', letter);
            var spanElement = $('<span>').addClass('letter').innerHTML = '<span class="letter">' + letter + '</span>';

            letterElement.append(spanElement);
        }

        var peopleList = $('<ul>').attr('id', 'users');
        $.each(data, function(index, person) {
            var peopleElement = $('<li>');
            //var peopleData = $('<a>').attr('href', '#').attr('data-uid', person.uid).html(person.sn + ' ' + person.givenName + '<br /><span>' + person.affiliation  + '</span>')
            // correction temporaire
            var peopleData = $('<a>').attr('href', '#').attr('data-uid', person.uid).html(person.sn + ' ' + person.givenName + '<br />')

            peopleElement.append(peopleData);
            peopleList.append(peopleElement);
        });

        letterList.append(letterElement);
        letterList.append(peopleList);

        $('#list').empty().append(letterList);

        fadeIn();
        return;
    }

    function renderUserInfos(data, eltarget){
        if($('#fiche2').length) $('#fiche2').remove();
        var container = $('<div>').attr('id', 'fiche2').addClass('fiche cms active');
        var inside = $('<div>').addClass('inside');
        //var title = $('<h2>').html(data.sn + ' ' + data.givenName + '<br><span>' + data.affiliation + '</span>');
        // correction temporaire
        var title = $('<h2>').html(data.sn + ' ' + data.givenName);
        var address = $('<div>').addClass('address');
        var addressTitle = $('<h3>').html('Coordonnées');
        var addressParagraph = $('<p>');
//Ancien code clever-age
//        if (data.supannEntiteAffectation) {
//            var addressService = $('<h3>').html(data.supannEntiteAffectation);
//            address.append(addressService);
//        }
//        if (data.ubxSite) {
//            var site = $('<h3>').html(data.ubxSite);
//            address.append(site);
//        }
//Fin ancien code clever-age
//Code Bruno
         if (data.supannEntiteAffectation) {
            var addressService = $('<h3>').html('Structure(s) : ');
            address.append(addressService);
            var addressServiceParagraph = $('<p>');
            address.append(addressServiceParagraph);
            var service = data.supannEntiteAffectation;
            if( typeof service === 'string') {
                addressServiceParagraph.append(service);
            }
            else {
                var index;
                for (index = 0; index < service.length; ++index) {
                    if (service[index]!='')
                        addressServiceParagraph.append(service[index] + '<br>');
                }
                addressServiceParagraph.append('<br>');
            }
        }

        if (data.ubxSite) {
            var site = $('<h3>').html('Site : ' + data.ubxSite);
            address.append(site);
        }
//Fin code Bruno
        address.append(addressTitle);

        if (data.telephoneNumber) {
            addressParagraph.append('<abbr title="Téléphone">Tél.</abbr> ' + data.telephoneNumber + '<br>');
        }

        var addressMail = $('<a>').attr('href', 'mailto:' + data.mail).html(data.mail);
        addressParagraph.append(addressMail);
        address.append(addressParagraph);

        inside.append(title);
        inside.append(address);
        if (data.path != undefined) {
            var ul = $('<ul>').addClass('cta');
            var li = $('<li>');
            li.html('<a href="' + data.path +'" class="btn">Voir sa fiche détaillée</a>');

            ul.append(li);
            inside.append(ul);
        }

        container.append(inside);
        eltarget.after(container);

        fadeIn();

        var viewport = $(document).width();
        if(viewport <= 499)
        {
            state = 'XXS';
        }
        else if(viewport <= 739)
        {
            state = 'XS';
        }
        else if(viewport > 739 && viewport <= 1023)
        {
            state = 'S';
        }
        else
        {
            state = 'M';
        }

        if(state == 'XXS' || state == 'XS')
        {
            $('#fiche2').slideToggle();
        }

        return;
    }

    $(selector).find('.filter').find('a').off('click').on('click', function(ev){
        ev.preventDefault();
        loadList($( this ).text());
    });

    $('#list').off('click').on('click', '#users a', function(ev){
        ev.preventDefault();
        loadUserInfos($(this));
    });

    $('.filter').off('submit').on('submit', 'form', function(ev){
        ev.preventDefault();
        var value = $(this).find('#index-personnes').val();
        if(value){
            search(value);
        }

        return false;
    });

    loadList('a');
}

$( document ).ready(function() {
    directory('#annuaire');
});
